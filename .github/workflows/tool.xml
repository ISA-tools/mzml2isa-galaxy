<tool id="test" name="test" version="0.0.1">
    <description>test</description>

    <stdio>
        <exit_code range="1:" />
    </stdio>
    
    <command detect_errors="exit_code">
        echo 'test' > "$test_out"
    </command>

    <inputs>
        <param name="test" type="text" label="test" help="test" />


    </inputs>
    <outputs>

    <data name="test_out" format="text" label="test_out"/>

    </outputs>

    <tests>
        <test>
            <param name="test" value="test" />
            <output name="test_out"  >
                 <assert_contents>
                         <has_text text="test" />
                 </assert_contents>
            </output>
        </test>
    </tests>
    <help><![CDATA[
test
    ]]></help>
    <citations>
        <citation type="doi">10.1002/0471250953.bi1413s53</citation>
    </citations>
</tool>




